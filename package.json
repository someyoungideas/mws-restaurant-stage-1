{
  "name": "mws-restaurant-stage-1",
  "version": "1.0.0",
  "description": "mobile web specialist certification course",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:clean && minify ./node_modules/whatwg-fetch/fetch.js -d ./build/vendor/ && minify ./node_modules/promise-polyfill/dist/polyfill.min.js -d ./build/vendor/ && minify ./node_modules/idb/lib/idb.js -d ./build/vendor/",
    "build:clean": "rimraf ./build/vendor",
    "js": "npm run js:clean && minify js -d build/js",
    "js:clean": "rimraf build/js",
    "css": "npm run css:clean && postcss css/styles.css -u cssnano autoprefixer --autoprefixer.browsers \"> 5%\" -o build/css/styles.css",
    "css:clean": "rimraf build/css",
    "imagemin": "npm run imagemin:clean && node imagemin.js",
    "imagemin:clean": "rimraf build/images"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/someyoungideas/mws-restaurant-stage-1.git"
  },
  "keywords": [
    "Udacity",
    "Mobile",
    "Web",
    "Specialist"
  ],
  "author": "Chris Young",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/someyoungideas/mws-restaurant-stage-1/issues"
  },
  "homepage": "https://github.com/someyoungideas/mws-restaurant-stage-1#readme",
  "devDependencies": {
    "autoprefixer": "^8.5.1",
    "babel-minify": "^0.4.3",
    "cssnano": "^3.10.0",
    "imagemin": "^5.3.1",
    "imagemin-jpeg-recompress": "^5.1.0",
    "imagemin-webp": "^4.1.0",
    "mkdirp": "^0.5.1",
    "postcss-cli": "^5.0.0",
    "rimraf": "^2.6.2",
    "sharp": "^0.20.2",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "idb": "^2.1.2",
    "promise-polyfill": "^8.0.0",
    "uglify-js": "^3.3.27",
    "whatwg-fetch": "^2.0.4"
  }
}
